<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/aboutRes"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_res"
    android:orientation="vertical">

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/viewPager"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.viewpager2.widget.ViewPager2>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="353dp"
        android:layout_marginTop="304dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/totalMoney"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="56dp"
            android:background="@drawable/shape_edittext01"
            android:drawableTop="@drawable/baseline_money_24"
            android:drawablePadding="10dp"
            android:padding="10dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/totalOrder"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_marginStart="36dp"
            android:layout_marginBottom="56dp"
            android:background="@drawable/shape_edittext01"
            android:drawableTop="@drawable/baseline_fastfood_24"

            android:drawablePadding="10dp"
            android:padding="10dp"
            android:text=""
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/totalMoney"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="in month"
            app:layout_constraintBottom_toBottomOf="@+id/totalOrder"
            app:layout_constraintEnd_toEndOf="@+id/totalOrder"
            app:layout_constraintHorizontal_bias="0.604"
            app:layout_constraintStart_toStartOf="@+id/totalOrder"
            app:layout_constraintTop_toTopOf="@+id/totalOrder"
            app:layout_constraintVertical_bias="0.792"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="in month"
            app:layout_constraintBottom_toBottomOf="@+id/totalMoney"
            app:layout_constraintEnd_toEndOf="@+id/totalMoney"
            app:layout_constraintHorizontal_bias="0.635"
            app:layout_constraintStart_toStartOf="@+id/totalMoney"
            app:layout_constraintTop_toTopOf="@+id/totalMoney"
            app:layout_constraintVertical_bias="0.792"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>